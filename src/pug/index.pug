doctype html
html(lang="en")
	include _header
	body
		#wrapper
			.tarpas
			section#output
				#statutory
					#maininfo
						.left
							.umegaagrologo
								include _umegaagrologo
							p#address Metalo str. 5, Utena,#[br] LT-28216, Lithuania #[br] www.umegaagro.com
							span#categoryl.label Category:
							span#categoryf.field {{currentTrailer[1].category}}
							span#typel.label Type:
							span#typef.field SPC20
							span#variantl.label Variant:
							span#variantf.field L
							span#versionl.label Version:
							span#versionf.field -
							span#productionl.label Manufactured:
							span#productionf.field 2019
							span#weightl.label Weight:
							span#weightf.field.kg 7750
							span#payloadl.label Payload:
							span#payloadf.field.kg 14250
							span#techPayloadl.label Tech. payload:
							span#techPayloadf.field.kg 19500
						.right
							span.umeganame Umega Group, AB
							span#homologation.field e32*167/2013*00021*00
							span#vin.field UMESPC20CJ0004578
							.columns
								.leftins
									span#country.field.country UA
									span#rFullWeight.field.kg 22000
									.cont
										span.prepoint 0:
										span#rpoint0.point.field.kg 4000
									.cont
										span.prepoint 1:
										span#rpoint1.point.field.kg 9000
									.cont
										span.prepoint 2:
										span#rpoint2.point.field.kg 9000
									.cont
										span.prepoint 3:
										span#rpoint3.point.field.kg -
								.rightins
									span.country.field &nbsp;
									span#fFullWeight.field.kg 27250
									.cont
										span.prepoint 0:
										span#fpoint0.point.field.kg 4000
									.cont
										span.prepoint 1:
										span#fpoint1.point.field.kg 11625
									.cont
										span.prepoint 2:
										span#fpoint2.point.field.kg 11625
									.cont
										span.prepoint 3:
										span#fpoint3.point.field.kg -
							.secondTrailer
								span.narrow.heading &nbsp;
								span.heading T-1
								span.heading T-2
								span.heading T-3
								span.narrow.heading B-1:
								span#t1b1.field 1500
								span#t2b1.field -
								span#t3b1.field -
								span.narrow.heading B-2:
								span#t1b2.field 8000
								span#t2b2.field -
								span#t3b2.field -
								span.narrow.heading B-3:
								span#t1b3.field 18000
								span#t2b3.field -
								span#t3b3.field -
								span.narrow.heading B-4:
								span#t1b4.field 18000
								span#t2b4.field -
								span#t3b4.field -
					footer
						span.iso ISO 9001
						span.ec EC
			section#input
				select
					option(
						v-for="tip in trailerTypes"
						v-bind:key="tip.id"
						) {{tip.name}}
			.tarpas
		script(src="js/scripts.js")